---
title: Experience
---
<article id="resume">

  <section id="skills">
    <h2>Skills</h2>
    <section id="technical-skills">
      <h3>Technical</h3>
      <ul class="no-bullets-list">
        {% for skill in resume.skills.technical %}
          <li>{{skill}}</li>
        {% endfor %}
      </ul>
    </section>
    <section id="design-skills">
      <h3>Design</h3>
      <ul class="no-bullets-list">
        {% for skill in resume.skills.design %}
          <li>{{skill}}</li>
        {% endfor %}
      </ul>
    </section>
    <section id="other-skills">
      <h3>Technical</h3>
      <ul class="no-bullets-list">
        {% for skill in resume.skills.other %}
          <li>{{skill}}</li>
        {% endfor %}
      </ul>
    </section>
  </section>

  <section id="affiliations">
    {% import "components/experience/affiliation-card.njk" as affiliationCard %}
    <h2>Professional Affiliations and Community Involvement</h2>
    {% for role in resume.affiliations %}
      {{ affiliationCard.card(role)}}
    {% endfor %}
  </section>

    <section id="experience">
    {% import "components/experience/experience-card.njk" as experienceCard %}
    <h2>Experience</h2>
    {% for role in resume.experience %}
      {{ experienceCard.card(role)}}
    {% endfor %}
  </section>


</article>